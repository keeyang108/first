<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.extra.first.dao.SupervisorDao">

    <insert id="addSupervisor" parameterType="com.extra.first.pojo.Supervisor">
         insert into t_supervisor
         (supervisor_name,supervisor_phone,password,sex,birthday,qq,email,head_image_path,create_time)
         VALUES
         (#{supervisor.supervisorName},#{supervisor.supervisorPhone},#{supervisor.password},#{supervisor.sex},
          #{supervisor.birthday},#{supervisor.qq},#{supervisor.email},#{supervisor.headImagePath,},now()
         );
    </insert>

    <!--<select id="checkUser" resultMap="com.extra.first.pojo.Supervisor" parameterType="com.extra.first.pojo.Supervisor">-->
    <!--select id,supervisor_name,password  from t_supervisor where supervisor_name=#{supervisor.supervisorName}-->
    <!--and password=#{supervisor.password};-->
  <!--</select>-->

    <select id="checkUser" resultType="java.util.Map" parameterType="com.extra.first.pojo.Supervisor">
        select id,supervisor_name,password  from t_supervisor where supervisor_name=#{supervisor.supervisorName}
        and password=#{supervisor.password};
    </select>

    <select id="selectUserByUserName" resultType="com.extra.first.pojo.Supervisor">
        select * from t_supervisor where supervisor_name = #{userName}
    </select>
    
</mapper>