<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.extra.first.dao.ProvinceDao">
    <insert id="addProvince" parameterType="String">
        insert into t_province(province_name) VALUES (#{provinceName});
    </insert>

    <select id="listProvince" resultType="com.extra.first.pojo.Province">
        select id,province_name from t_province limit #{offset},#{limit};
    </select>

</mapper>